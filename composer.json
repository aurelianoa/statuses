{
    "name": "stacht/statuses",
    "description": "Polymorphic Laravel package for status managemen",
    "license": "MIT",
    "authors": [{
        "name": "Aureliano Alarcon Smith",
        "email": "aureliano.alarcon@gmail.com"
    }],
    "homepage": "https://github.com/stacht/statuses",
    "keywords": ["Laravel", "Statuses", "Stacht"],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.1",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.8.0|^4.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Stacht\\Statuses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stacht\\Statuses\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stacht\\Statuses\\StatusesServiceProvider"
            ]
        }
    }
}
